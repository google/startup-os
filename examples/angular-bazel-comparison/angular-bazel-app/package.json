{
    "name": "example",
    "version": "0.0.1",
    "description": "Demo of bazel rules for angular",
    "license": "Apache 2.0",
    "dependencies": {
        "@ngrx/store": "6.1.0",
        "rxjs": "6.2.2",
        "tslib": "1.9.3",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular/compiler": "6.1.3",
        "@angular/compiler-cli": "6.1.3",
        "@angular/core": "6.1.3",
        "@bazel/benchmark-runner": "0.1.0",
        "@bazel/ibazel": "0.5.0",
        "@bazel/typescript": "0.16.1",
        "@types/jasmine": "2.8.8",
        "@types/node": "6.0.116",
        "clang-format": "1.2.4",
        "husky": "0.14.3",
        "protractor": "5.4.0",
        "tsickle": "0.32.1",
        "typescript": "2.9.2"
    },
    "scripts": {
        "postinstall": "ngc -p postinstall.tsconfig.json",
        "serve": "ibazel run //src:devserver",
        "prebuildifier": "bazel build @com_github_bazelbuild_buildtools//buildifier",
        "buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"./node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier",
        "format": "git-clang-format",
        "precommit": "check-clang-format \"yarn format\""
    }
}
