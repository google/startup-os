{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular-cli"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "deployUrl": "",
      "assets": ["assets"],
      "index": "bootstrap/index.html",
      "main": "bootstrap/angular.bootstrap.ts",
      "test": "dev/unit/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "polyfills": "dev/polyfills.ts",
      "prefix": "app",
      "environmentSource": "bootstrap/environments/environment.ts",
      "environments": {
        "dev": "bootstrap/environments/environment.ts",
        "prod": "bootstrap/environments/environment.prod.ts"
      },
      "styles": [
        "styles/angular.scss"
      ],
      "scripts": []
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "serve": {
      "port": 8000
    },
    "component": {}
  }
}
